function [g, Mexp, l] = M_exp(expData)
l = linspace(0,8*0.0127);
    initialVec = expData(1,2:9); %pulling initial values from expData
    T0 = initialVec(1);
    x = 1:length(initialVec); % creating thermocouples of space 1
    sep = 0.0127; % separation b/w TC
    p = polyfit(sep*x,initialVec,1); % getting the LOBF across thermocouples
    Mexp = p(1); %slope of IC
    g = Mexp*l + p(2); %equation of LOBF
end